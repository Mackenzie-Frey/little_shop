<% @users.each do |user| %>
  <div id="user-<%=user.id%>">
      <%= link_to(user.name, admin_user_path(user)) %> | Registered at <%= user.created_at.to_date %>
      <% if user.enabled? %>
        <%= button_to("Disable", admin_disable_user_path(user), method: :put) %>
      <% else %>
        <%= button_to("Enable", admin_enable_user_path(user), method: :put) %>
      <% end %>
  </div>
<% end %>
